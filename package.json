{
  "name": "emotitone-solfege",
  "version": "1.0.0",
  "description": "An interactive music theory web app that teaches solfege through emotional experiences",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5175",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run src/__tests__/e2e/",
    "test:e2e:ui": "vitest --ui src/__tests__/e2e/",
    "test:e2e:watch": "vitest src/__tests__/e2e/"
  },
  "dependencies": {
    "@tonaljs/tonal": "^4.10.0",
    "gsap": "^3.12.5",
    "lucide-vue-next": "^0.523.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^4.4.0",
    "tone": "^15.0.4",
    "vue": "^3.4.0",
    "vue-sonner": "^2.0.1"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "vite": "^4.5.0",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^1.8.25",
    "workbox-window": "^7.3.0"
  }
}
